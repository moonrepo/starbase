---
source: crates/shell/src/shells/zsh.rs
expression: "Zsh::default().format_hook(hook).unwrap()"
---
export __ORIG_PATH="$PATH"

starbase() {
  trap '' SIGINT
  output=$(starbase hook zsh)
  if [ -n "$output" ]; then
    eval "$output";
  fi
  trap - SIGINT
}

typeset -ag chpwd_functions
if (( ! ${chpwd_functions[(I)starbase]} )); then
  chpwd_functions=(starbase $chpwd_functions)
fi
